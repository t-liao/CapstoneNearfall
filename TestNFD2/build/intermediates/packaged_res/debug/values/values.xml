<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="error_information">
        <item>Sucess</item>
        <item> : Client is not connected. Check your network connection and verify if the client is up and running.\n\n</item>
        <item> : Server is not connected. Check your network connection and verify if the Remote address and Remote port are correct.\n\n</item>
        <item> : TCP Connection established successfully.\n\n</item>
        <item> : Port number #argument# specified in the mask is used by another application. Change the port number and deploy the model again.\n\n</item>
        <item> : Cannot resolve the host name #argument# to IP address. Change the Remote address block parameter and deploy the model again.\n\n</item>
        <item> : Server could not be created. Restart the app.\n\n</item>
        <item> : Client could not be created. Restart the app.\n\n</item>       
    </string-array>
    <color name="colorAccent">#029FCF</color>
    <color name="colorPrimary">#DCE6EB</color>
    <color name="titleBackground">#fafafa</color>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="appbar_padding_top">8dp</dimen>
    <dimen name="fab_margin">16dp</dimen>
    <string name="app_name">TestNFD2</string>
    <string name="empty_hint">Log is empty</string>
    <string name="tcp_title">TCP Logs</string>
    <string name="thingspeak_title">ThingSpeak Logs</string>
    <style name="MWAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="android:textColorSecondary">@color/dim_foreground_material_dark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="MWAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <declare-styleable name="Croller">
        <attr format="boolean" name="enabled"/>
        <attr format="float" name="start_progress"/>
        <attr format="string" name="label"/>
        <attr format="color" name="back_circle_color"/>
        <attr format="color" name="back_circle_disable_color"/>
        <attr format="color" name="main_circle_color"/>
        <attr format="color" name="main_circle_disable_color"/>
        <attr format="color" name="indicator_color"/>
        <attr format="color" name="indicator_disable_color"/>
        <attr format="color" name="progress_primary_color"/>
        <attr format="color" name="progress_primary_disable_color"/>
        <attr format="color" name="progress_secondary_color"/>
        <attr format="color" name="progress_secondary_disable_color"/>
        <attr format="dimension" name="label_size"/>
        <attr format="color" name="label_color"/>
        <attr format="string" name="label_font"/>
        <attr format="enum" name="label_style">
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="italic" value="2"/>
            <enum name="bold_italic" value="3"/>
        </attr>
        <attr format="color" name="label_disabled_color"/>
        <attr format="float" name="indicator_width"/>
        <attr format="boolean" name="is_continuous"/>
        <attr format="float" name="progress_primary_circle_size"/>
        <attr format="float" name="progress_secondary_circle_size"/>
        <attr format="float" name="progress_primary_stroke_width"/>
        <attr format="float" name="progress_secondary_stroke_width"/>
        <attr format="integer" name="sweep_angle"/>
        <attr format="integer" name="start_offset"/>
        <attr format="integer" name="max"/>
        <attr format="integer" name="min"/>
        <attr format="float" name="min_tick_value"/>
        <attr format="float" name="max_tick_value"/>
        <attr format="float" name="tick_interval"/>
        <attr format="integer" name="sid"/>
        <attr format="float" name="main_circle_radius"/>
        <attr format="float" name="back_circle_radius"/>
        <attr format="float" name="progress_radius"/>
        <attr format="boolean" name="anticlockwise"/>
    </declare-styleable>
    <declare-styleable name="GaugeView">
        <attr format="boolean" name="showNeedle"/>
        <attr format="boolean" name="showRanges"/>
        <attr format="boolean" name="showTextGauge"/>

        <attr format="float" name="needleWidth"/>
        <attr format="float" name="needleHeight"/>

        <attr format="float" name="scalePosition"/>
        <attr format="float" name="scaleStartValue"/>
        <attr format="float" name="scaleEndValue"/>
        <attr format="float" name="scaleStartAngle"/>
        <attr format="float" name="scaleEndAngle"/>

        <attr format="integer" name="divisions"/>
        <attr format="integer" name="subdivisions"/>

        <attr format="string|reference" name="textValue"/>
        <attr format="color" name="textValueColor"/>
        <attr format="float" name="textValueSize"/>
        <attr format="string|reference" name="textUnit"/>
        <attr format="color" name="textUnitColor"/>
        <attr format="float" name="textUnitSize"/>
        <attr format="color" name="textShadowColor"/>

        <attr format="color" name="innerCircleColor"/>
        <attr format="color" name="outerCircleColor"/>

    </declare-styleable>
</resources>